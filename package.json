{
    "name": "vscode-taskexplorer",
    "version": "1.21.0",
    "displayName": "Task Explorer",
    "description": "Manage tasks for npm, vscode, ant, gradle, grunt, gulp, batch, bash, make, python, perl, powershell, ruby, and nsis",
    "icon": "res/gears-r-colors.png",
    "publisher": "spmeesseman",
    "license": "MIT",
    "homepage": "https://github.com/spmeesseman/vscode-taskexplorer/blob/master/README.md",
    "author": {
        "name": "Scott Meesseman"
    },
    "bugs": {
        "url": "https://github.com/spmeesseman/vscode-taskexplorer/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/spmeesseman/vscode-taskexplorer.git"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "galleryBanner": {
        "color": "#EEEEEE",
        "theme": "light"
    },
    "keywords": [
        "task",
        "tasks",
        "npm",
        "ant",
        "makefile",
        "multi-root ready",
        "c c++",
        "grunt",
        "gulp",
        "gradle",
        "tsc",
        "nsis",
        "batch",
        "bash"
    ],
    "main": "./out/extension",
    "activationEvents": [
        "*"
    ],
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "files": [
        "res",
        "src/*.js",
        "src/common/*.js",
        "types"
    ],
    "scripts": {
        "package": "vsce package",
        "publish": "vsce publish",
        "vscode:prepublish": "npm run build",
        "build": "npx tsc -p ./",
        "compile": "npx tsc -p ./",
        "semantic-release": "semantic-release",
        "semantic-release-no-ci": "semantic-release --no-ci",
        "semantic-release-dry": "semantic-release --dry-run",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "commit": "npx git-cz"
    },
    "dependencies": {
        "is-utf8": "^0.2.1",
        "jsonc-parser": "^2.2.0",
        "minimatch": "^3.0.4",
        "semver": "^6.3.0",
        "vscode-nls": "^4.1.1",
        "xml2js": "^0.4.22"
    },
    "devDependencies": {
        "@semantic-release/changelog": "^3.0.6",
