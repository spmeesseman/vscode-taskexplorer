                "key": "alt+r t",
                "when": "config.taskExplorer.enableExplorerView"
            },
            {
                "command": "taskExplorerSideBar.runLastTask",
                "key": "alt+r t",
                "when": "config.taskExplorer.enableSideBar"
            }
        ],
