        "@types/mocha": "^5.2.7",
        "@types/node": "^12.0.1",
        "@types/semver": "^7.1.0",
        "@types/xml2js": "^0.4.5",
        "codecov": "^3.6.1",
        "commitizen": "^4.0.0",
        "conventional-changelog-spm": "^1.2.3",
        "cz-conventional-changelog": "^3.0.0",
        "decache": "^4.5.1",
        "eslint-plugin-prettier": "^3.1.1",
        "glob": "^7.1.6",
        "grunt": "^1.0.4",
        "gulp": "^4.0.2",
        "iconv-lite": "^0.5.0",
        "istanbul": "^0.4.5",
        "jschardet": "^2.1.1",
